    <changeSet id="loadDataStaticMapping" author="jbu" context="staticdata">

        <loadData encoding="UTF-8"
                  file="config/liquibase/static_mapping.csv"
                  separator=";"
                  tableName="static_mapping"/>

    </changeSet>
</databaseChangeLog>
